<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <!-- Data for Pos001 -->
   <bean class="com.vmware.vc.HostSriovConfig" id="sriovPos001">
      <property name="sriovEnabled">
         <value>true</value>
      </property>
      <property name="numVirtualFunction">
         <value>3</value>
      </property>
   </bean>
   <bean class="com.vmware.vc.HostSriovConfig" id="sriovPos001_1">
      <property name="sriovEnabled">
         <value>false</value>
      </property>
      <property name="numVirtualFunction">
         <value>0</value>
      </property>
   </bean>
   <!-- End for Pos001  -->

   <!-- Data for Pos002 -->
   <bean class="com.vmware.vc.HostSriovConfig" id="sriovPos002">
      <property name="sriovEnabled">
         <value>true</value>
      </property>
      <property name="numVirtualFunction">
         <value>2</value>
      </property>
   </bean>
   <bean class="com.vmware.vc.HostSriovConfig" id="sriovPos002_1">
      <property name="sriovEnabled">
         <value>true</value>
      </property>
      <property name="numVirtualFunction">
         <value>4</value>
      </property>
   </bean>
   <!-- End for Pos002  -->
</beans>