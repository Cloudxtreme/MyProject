<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <!--  Data for Neg001 -->
   <bean class="com.vmware.vc.HostSriovConfig" id="sriovNeg001">
      <property name="sriovEnabled">
         <value>true</value>
      </property>
      <property name="numVirtualFunction">
         <value>-1</value>
      </property>
   </bean>
   <!-- End for Neg001  -->

   <!--  Data for Neg002 -->
   <bean class="com.vmware.vc.VirtualPCIPassthroughDeviceBackingInfo"
      id="pfbakcingNeg002">
      <property name="id">
         <value>00:00.00</value>
      </property>
      <property name="deviceId">
         <value>123</value>
      </property>
      <property name="systemId">
         <value>1123</value>
      </property>
      <property name="vendorId">
         <value>0</value>
      </property>
      <property name="deviceName">
         <value>qwert#$1</value>
      </property>
   </bean>
   <!-- End for Neg002  -->
</beans>